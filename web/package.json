{
  "name": "lifecell-ai",
  "version": "1.0.0",
  "type": "module",
  "description": "A project setup with Webpack, TailwindCSS, DaisyUI, DataTables, Highlight.js, HTMX, and jQuery.",
  "keywords": [
    "webpack",
    "tailwindcss",
    "daisyui",
    "datatables",
    "highlight.js",
    "htmx",
    "jquery",
    "marked"
  ],
  "scripts": {
    "copy:styles": "cp node_modules/datatables.net-dt/css/dataTables.dataTables.min.css static/dist/datatables.css && cp node_modules/highlight.js/styles/base16/dracula.css static/dist/highlight.css",
    "copy:ext": "cp node_modules/htmx.org/dist/ext/debug.js static/dist/ && cp node_modules/htmx.org/dist/ext/json-enc.js static/dist/",
    "build:tw": "tailwindcss -i static/css/tailwind.css -o static/dist/tailwind.css -m",
    "build:tw:dev": "tailwindcss -i static/css/tailwind.css -o static/dist/tailwind.css -c tailwind.config.DEV.js",
    "tw:watch": "tailwindcss -i static/css/tailwind.css -o static/dist/tailwind.css --watch",
    "wp:watch": "webpack --watch --progress",
    "build": "export NODE_ENV=production && webpack --progress && npm run copy:styles && npm run build:tw && npm run copy:ext && mv static/main.html views/layouts/",
    "_build": "export NODE_ENV=production && webpack --progress && npm run copy:styles && npm run build:tw && npm run copy:ext",
    "build:dev": "export NODE_ENV=development && webpack --progress --mode development -c webpack.config.dev.cjs && npm run build:tw:dev && npm run copy:ext && mv static/main.html views/layouts/",
    "serve": "http-server -o -c-1 -i",
    "start:w": "webpack serve --open",
    "test": "jest"
  },
  "author": "Dmytro Mykolenko",
  "license": "ISC",
  "dependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "browserify": "^17.0.0",
    "clipboard": "^2.0.11",
    "daisyui": "^4.12.2",
    "datatables.net": "^2.0.8",
    "datatables.net-dt": "^2.0.8",
    "highlight.js": "^11.9.0",
    "htmx-ext-debug": "^2.0.0",
    "htmx-ext-json-enc": "^2.0.0",
    "htmx.org": "^1.9.12",
    "jquery": "^3.7.1",
    "jquery-ui": "^1.13.3",
    "jquery-ui-dist": "^1.13.3",
    "marked": "^13.0.0",
    "marked-highlight": "^2.1.2",
    "postcss-loader": "^8.1.1",
    "theme-change": "^2.5.0",
    "typed.js": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@biomejs/biome": "1.8.3",
    "autoprefixer": "^10.4.19",
    "babelify": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "font-config-webpack-plugin": "^2.0.3",
    "go-template": "^0.1.7",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss": "^8.4.38",
    "prettier": "3.3.2",
    "prettier-plugin-go-template": "^0.0.15",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.4",
    "webpack": "^5.92.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-manifest-plugin": "^5.0.0"
  }
}
